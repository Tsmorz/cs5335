function opt()

        R = eye(2);
        
        T = 20;
        x0 = [2; 2];
        x_goal = [4; 0];

        A = []
        b = []
        Aeq = [ones(1, T); ones(1, T)]
        beq = [x_goal-x0]

        x(:, 0) = x0;
        x(:, T) = x_goal;
        x = fmincon(@cost, x0, A, b, Aeq, beq)

end

% cost function to minimize
function f = cost(x)

        u = x(2)-x(1);
        R = eye(2);
        f = u'*R*u;

end